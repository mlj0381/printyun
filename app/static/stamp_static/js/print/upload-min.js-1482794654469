var pubObject={closeWindow:function(){WeixinJSBridge.invoke("closeWindow",{},function(a){})},formAjax:function(url,fromId){startWait();var formData=$("#"+fromId).serialize();$.ajax({type:"POST",url:url,cache:false,data:formData,dataType:"json",success:function(data){closeWait();if(data.toString().indexOf("window.top.location.href")>0){alertWarningCallBackDiv();return false}data=data.resultInfo;if(data.rsType==TYPE_RESULT.SUCCESS){alertSuccess("\u63d0\u4ea4\u6210\u529f\uff0c\u53ef\u53bb\u6253\u5370\u5217\u8868\u67e5\u770b\uff01");location.reload()}else{if(data.rsType==TYPE_RESULT.WARN){var rsData=eval("("+data.rsData+")");$(".errMsg").show();$(".errMsg").text(rsData.ErrMsg)}else{alertFail("\u7cfb\u7edf\u5f02\u5e38\uff01");return false}}}})}};function unbind_click(a){$(a).unbind("click")}function upload(b,a){var c=new XMLHttpRequest();c.open("POST",a,true);c.addEventListener("load",uploadComplete,false);c.send(b)}function uploadProgress(a){if(a.lengthComputable){var b=Math.round(a.loaded*100/a.total);document.getElementById("progressNumber").innerHTML=b.toString()+"%"}else{document.getElementById("progressNumber").innerHTML="unable to compute"}}function getWidthAndHeight(d,c,b,a){if(c>=d){d*=a/c;c=a}else{c*=b/d;d=b}return new Array(d,c)};
